{
  "name": "item-info-service",
  "version": "1.0.0",
  "description": "Exposes a REST service which gets information about an item, including name and pricing information, based on an item ID. Also allows pricing updates.",
  "main": "build/index.js",
  "type": "module",
  "scripts": {
    "test": "./node_modules/.bin/jest",
    "build": "rm -rf build && ./node_modules/.bin/tsc",
    "package": "./node_modules/.bin/pkg --out-path ./dist ./build/index.js",
    "lint": "eslint . --ext .ts",
    "start": "node ./build/index.js",
    "start:debug": "node ./build/index.js --debug"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/invictus125/item-info-service.git"
  },
  "keywords": [
    "REST",
    "pricing",
    "shopping",
    "node",
    "typescript"
  ],
  "author": "Ryan David",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/invictus125/item-info-service/issues"
  },
  "homepage": "https://github.com/invictus125/item-info-service#readme",
  "dependencies": {
    "express": "^4.17.1",
    "lodash": "^4.17.19",
    "nosql": "^6.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/types": "^7.10.5",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.7",
    "@types/lodash": "^4.14.158",
    "@types/node": "^14.0.25",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "eslint": "^7.5.0",
    "jest": "^26.1.0",
    "pkg": "^4.4.9",
    "typescript": "^3.9.7"
  }
}
